<!DOCTYPE html>
<html>

<head>
  <!--Import Google Icon Font-->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!--Import materialize.css-->

  <link type="text/css" rel="stylesheet" href="css/materialize.min.css" media="screen,projection" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
  <link rel="stylesheet" href="/stylesheets/style.css">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Crimson+Pro">
  <style>
    body {
      font-family: 'Crimson Pro', serif;
      font-size: 42px;
    }
  </style>
  <!--Let browser know website is optimized for mobile-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <script>
    document.addEventListener('DOMContentLoaded', function () {
      var elems = document.querySelectorAll('.sidenav');
      var instances = M.Sidenav.init(elems);
    });
  </script>
</head>

<header>
  <nav>
    <div class="nav-wrapper">
      <a href="/" class="brand-logo">iMemeit</a>
      <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
      <ul class="right hide-on-med-and-down">
        <!-- Button to open the modal login form -->

        {{#if loggedIn }}
        <li><a href="/dashboard">Dashboard</a></li>
        </li>
        <li><a id="logout" href="/"><i class="material-icons">exit_to_app</i></a></li>
        {{else}}
        <li><a href="/login">Login</a></li>
        {{/if}}
      </ul>
    </div>
  </nav>
  <ul class="sidenav" id="mobile-demo">
    {{#if loggedIn }}
    <li><a href="/dashboard"><i class="material-icons">dashboard</i>Dashboard</a></li>
    <li><a id="logout"><i class="material-icons">exit_to_app</i>Logout</a></li>
    {{else}}
    <li><a href="/login">Login</a></li>
    {{/if}}
  </ul>

</header>

<body>
  {{{body}}}
</body>

<!--JavaScript at end of body for optimized loading-->
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
{{!--
<script src="/javascript/image.js"></script> --}}
{{#if loggedIn}}
<script src="/javascript/logout.js"></script>
<script src="/javascript/upvote.js"></script>
{{/if}}



</html>